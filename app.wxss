/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.view_button {
  margin: 10rpx 24rpx;
  background: #2a7efb;
  display: flex;
  height: 88rpx;
  color: #fff;
  align-items: center;
  justify-content: center;
  border-radius: 8rpx;
}

.view_button_hover {
  background: #095dc4;
}